<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard </title>
    <!-- ======= Styles ====== -->
    <link rel="stylesheet" href="/styles/entreDash.css" type="text/css">
</head>

<body>
    <!-- =============== Navigation ================ -->
    <div>
    <div class="container">
       
        <div class="navigation">
            <ul>
                <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="logo-apple"></ion-icon>
                        </span>
                        <span class="title"><%= entr.name %></span>
                    </a>
                </li>

                <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="home-outline"></ion-icon>
                        </span>
                        <span class="title">Dashboard</span>
                    </a>
                </li>

                <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="people-outline"></ion-icon>
                        </span>
                        <span class="title">Projects</span>
                    </a>
                </li>

                <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="people-outline"></ion-icon>
                        </span>
                        <span class="title">Investors</span>
                    </a>
                </li>

                <!-- <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="help-outline"></ion-icon>
                        </span>
                        <span class="title">Help</span>
                    </a>
                </li> -->

                <li>
                    <a href="/">
                        <span class="icon">
                            <ion-icon name="settings-outline"></ion-icon>
                        </span>
                        <span class="title">Home</span>
                    </a>
                </li>

                <li>
                    <a href="#">
                        <span class="icon">
                            <ion-icon name="lock-closed-outline"></ion-icon>
                        </span>
                        <span class="title">Password</span>
                    </a>
                </li>

                <li>
                    <a href="/logout">
                        <span class="icon">
                            <ion-icon name="log-out-outline"></ion-icon>
                        </span>
                        <span class="title">Sign Out</span>
                    </a>
                </li>
            </ul>
        </div>

        <!-- ========================= Main ==================== -->
        <div class="main">
            <div class="topbar">
                <div class="toggle">
                    <ion-icon name="menu-outline"></ion-icon>
                </div>

                <!-- <div class="search">
                    <label>
                        <input type="text" placeholder="Search here">
                        <ion-icon name="search-outline"></ion-icon>
                    </label>
                </div> -->

                <div class="user">
                    <img src="assets/imgs/customer01.jpg" alt="">
                </div>
            </div>

            <!-- ======================= Cards ================== -->
            <div class="cardBox">
                <div class="card">
                    <div>
                        <div class="numbers"><%= projects.length %></div>
                        <div class="cardName">Projects</div>
                    </div>

                    <!-- <div class="iconBx">
                        <ion-icon name="eye-outline"></ion-icon>
                    </div> -->
                </div>

                <div class="card">
                    <div>
                        <div class="numbers"><%= entr.rating %> </div>
                        <div class="cardName">Ratings</div>
                    </div>

                    <!-- <div class="iconBx">
                        <ion-icon name="cart-outline"></ion-icon>
                    </div> -->
                </div>

                <div class="card">
                    <div>
                        <div class="numbers">0</div>
                        <div class="cardName">Locked Projects</div>
                    </div>

                    <!-- <div class="iconBx">
                        <ion-icon name="chatbubbles-outline"></ion-icon>
                    </div> -->
                </div>

                <div class="card">
                    <div>
                        <div>
                            <div class="cardName"><a href="/project/createProject" class="btn" style="text-decoration:none">Create Project</a></div>
                        </div>
                    </div>

                    <!-- <div class="iconBx">
                        <ion-icon name="cash-outline"></ion-icon>
                    </div> -->
                </div>
            </div>

            <!-- ================ Order Details List ================= -->
            <div class="details">
                <div class="recentOrders">
                    <div class="cardHeader">
                        <h2>Recent Projects</h2>
                        <a href="#" class="btn">View All</a>
                    </div>

                    <table>
                        <thead>
                            <tr>
                                <td>Name</td>
                                <td>Price</td>
                                <td>Deal</td>
                                <td>Status</td>
                            </tr>
                        </thead>

                        <tbody>
                            <% projects.forEach(function(project){ %>
                            <tr>
                                <td><%= project.name %></td>
                                <td><%= project.basicDeal.amount %></td>
                                <td><%= project.basicDeal.equity %></td>
                                <td><span class="status delivered"><a href="/project/entrProject/<%= project._id %> ">See More</a></span></td>
                            </tr>
                            <%});%>
                        </tbody>
                    </table>
                </div>

                <!-- ================= New Customers ================ -->
                <div class="recentCustomers">
                    <div class="cardHeader">
                        <h2>Recent Investors</h2>
                    </div>

                    <table>
                        <tr>
                            <td width="60px">
                                <div class="imgBx"><img src="assets/imgs/customer02.jpg" alt=""></div>
